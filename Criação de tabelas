CREATE TABLE [TABELA DE CLIENTES]
(

[CPF] [VARCHAR] (11),

[NOME] [VARCHAR] (100), 

[ENDEREÇO 1] [VARCHAR] (150), 

[ENDEREÇO 2] [VARCHAR] (150), 

[BAIRRO] [VARCHAR] (50), 

[CIDADE] [VARCHAR] (50),

[ESTADO] [VARCHAR] (2), 

[CEP] [VARCHAR] (8), 

[DATA DE NASCIMENTO] [DATE] ,

[IDADE] [SMALLINT],

[SEXO] [VARCHAR] (1),

[LIMITE DE CRÉDITO] [MONEY],

[VOLUME DE COMPRA] [FLOAT], 

[PRIMEIRA COMPRA] [BIT]


)
